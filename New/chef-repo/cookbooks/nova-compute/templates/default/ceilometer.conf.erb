[DEFAULT]

rpc_backend = rabbit
auth_strategy = keystone

[keystone_authtoken]

auth_uri = http://<%= node['telemetry']['management_ip'] %>:5000
auth_url = http://<%= node['telemetry']['host_ip'] %>:35357
auth_plugin = password
project_domain_id = default
user_domain_id = default
project_name = service
username = <%= node['telemetry']['db_user'] %>
password = <%= node['telemetry']['db_pass'] %>

[oslo_messaging_rabbit]

rabbit_host = <%= node['telemetry']['host_ip'] %>
rabbit_userid = openstack
rabbit_password = rabbit

[service_credentials]

os_auth_url = http://<%= node['telemetry']['host_ip'] %>:35357/v2.0
os_username =<%= node['telemetry']['db_user'] %>
os_tenant_name = service
os_password = <%= node['telemetry']['db_pass'] %>
os_endpoint_type = internalURL
os_region_name = RegionOne