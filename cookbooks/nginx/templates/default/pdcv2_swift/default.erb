server {
  listen 443;
  server_name swift-bionimbus-pdc.opensciencedatacloud.org;

  access_log  /var/log/nginx/swift-bionimbus-pdc.opensciencedatacloud.org.access.log;
  error_log   /var/log/nginx/swift-bionimbus-pdc.opensciencedatacloud.org.error.log;

  ssl on;
  gzip off;

  client_max_body_size 0;

  ssl_certificate /etc/swift/swift-bionimbus-pdc.opensciencedatacloud.org.crt;
  ssl_certificate_key /etc/swift/swift-bionimbus-pdc.opensciencedatacloud.org.key;

  ssl_session_timeout 5m;

      ssl_protocols        SSLv3 TLSv1 ;
      ssl_ciphers RC4:HIGH:!aNULL:!MD5;
      ssl_prefer_server_ciphers on;
      keepalive_timeout    60;
      proxy_ssl_session_reuse off;
    #ssl_session_cache    shared:SSL:10m;
    #ssl_session_timeout  10m;


  location / {
    proxy_pass http://127.0.0.1:8080;
  }

}

